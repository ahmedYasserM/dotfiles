// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "alt+j",
    "command": "list.focusDown",
    "when": "listFocus && !inputFocus && !editorTextFocus"
  },
  {
    "key": "alt+k",
    "command": "list.focusUp",
    "when": "listFocus && !inputFocus && !editorTextFocus"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "alt+j",
    "command": "ctrl+j",
    "when": "terminalFocus"
  },
  {
    "key": "alt+k",
    "command": "ctrl+k",
    "when": "terminalFocus"
  },
  {
    "key": "alt+r",
    "command": "ctrl+r",
    "when": "terminalFocus"
  },
  {
    "key": "alt+m",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.navigateUp",
    "when": "!inQuickOpen && neovim.mode == normal"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.navigateDown",
    "when": "!inQuickOpen && neovim.mode == normal"
  },
  {
    "key": "alt+h",
    "command": "workbench.action.navigateLeft"
    // "when": "neovim.mode == normal"
  },
  {
    "key": "alt+l",
    "command": "workbench.action.navigateRight",
    "when": "neovim.mode == normal"
  },
  {
    "key": "alt+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "alt+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "alt+.",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "shift+k",
    "command": "editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus && noevim.mode == normal"
  },
  {
    "key": "shift+l",
    "command": "cursorLineEnd",
    "when": "neovim.mode == normal && textInputFocus && !terminalFocus"
  },
  {
    "key": "shift+h",
    "command": "cursorLineStart",
    "when": "neovim.mode == normal && textInputFocus && !terminalFocus"
  },
  {
    "key": "alt+;",
    "command": "editor.action.inlineSuggest.commit",
    "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus && !suggestWidgetVisible"
  },
  {
    "key": "alt+o",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "alt+i",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.quickOpen"
  },
  {
    "key": "alt+n",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "f1",
    "command": "-workbench.action.showCommands"
  },
  {
    "key": "alt+shift+;",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "alt+;",
    "command": "editor.action.inlineSuggest.commit"
  },
  {
    "key": "shift+alt+h",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "shift+alt+l",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "alt+,",
    "command": "windsurf.prioritized.chat.open",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+l",
    "command": "-windsurf.prioritized.chat.open",
    "when": "!terminalFocus"
  },
  {
    "key": "alt+u",
    "command": "windsurf.prioritized.command.open"
  },
  {
    "key": "ctrl+i",
    "command": "-windsurf.prioritized.command.open",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+space",
    "command": "editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "ctrl+shift+space",
    "command": "-editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "ctrl+c",
    "command": "notebook.cell.clearOutputs",
    "when": "notebookCellEditable && notebookCellHasOutputs && notebookEditable && notebookEditorFocused && !inputFocus"
  },
  {
    "key": "alt+delete",
    "command": "-notebook.cell.clearOutputs",
    "when": "notebookCellEditable && notebookCellHasOutputs && notebookEditable && notebookEditorFocused && !inputFocus"
  },
  {
    "key": "ctrl+c",
    "command": "jupyter.interactive.clearAllCells"
  },
  {
    "key": "alt+enter",
    "command": "manim-sideview.renderNewScene"
  },
  {
    "key": "ctrl+' c",
    "command": "-manim-sideview.renderNewScene"
  }
]